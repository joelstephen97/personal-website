{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "node --max-old-space-size=12288 ./node_modules/nuxt/bin/nuxt.mjs dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "pwa:generate": "npx --yes @vite-pwa/assets-generator --preset minimal-2023 public/icon.svg",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier . --check",
    "format": "prettier --write .",
    "lintfix": "eslint . --fix && prettier --write ."
  },
  "dependencies": {
    "@dnd-kit/vue": "^0.3.2",
    "@emailjs/browser": "^4.4.1",
    "@huggingface/transformers": "^3.8.1",
    "@nuxt/image": "^2.0.0",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/sitemap": "7.6.0",
    "@tensorflow-models/body-pix": "^2.2.1",
    "@tensorflow/tfjs-backend-webgl": "^4.22.0",
    "@tensorflow/tfjs-core": "^4.22.0",
    "@vercel/analytics": "^1.3.1",
    "@vercel/speed-insights": "^1.0.12",
    "@vite-pwa/nuxt": "^0.10.0",
    "@vueuse/core": "^11.1.0",
    "dompurify": "^3.3.1",
    "lucide-vue-next": "^0.469.0",
    "nuxt": "^3.14.0",
    "spark-md5": "^3.0.2",
    "typed.js": "^2.1.0"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.7.0",
    "@types/dompurify": "^3.2.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "postcss": "^8.4.49",
    "prettier": "^3.4.1",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3"
  }
}
